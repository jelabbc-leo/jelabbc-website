        <fileExtensions>
          <remove fileExtension=".exe" />
          <remove fileExtension=".bat" />
          <remove fileExtension=".cmd" />
          <remove fileExtension=".com" />
          <remove fileExtension=".dll" />
          <remove fileExtension=".msi" />
          <remove fileExtension=".vbs" />
          <!-- <remove fileExtension=".js" /> COMENTADO: Los archivos .js son necesarios para la aplicaci?n -->
        </fileExtensions>
      </requestFiltering>
    </security>
    <!-- Configuraci?n de tipos MIME para archivos est?ticos -->
    <staticContent>
      <remove fileExtension=".js" />
      <remove fileExtension=".css" />
      <remove fileExtension=".json" />
      <remove fileExtension=".woff" />
      <remove fileExtension=".woff2" />
      <remove fileExtension=".svg" />
      <mimeMap fileExtension=".js" mimeType="application/javascript" />
      <mimeMap fileExtension=".css" mimeType="text/css" />
      <mimeMap fileExtension=".json" mimeType="application/json" />
      <mimeMap fileExtension=".woff" mimeType="application/font-woff" />
